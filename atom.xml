---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Alexandr Bizikov</title>
    <link href="http://bizikov.ru/atom.xml" rel="self"/>
    <link href="http://bizikov.ru/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://bizikov.ru/</id>
    <author>
        <name>Alexandr Bizikov</name>
        <email>alexandr@bizikov.ru</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://bizikov.ru{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://bizikov.ru{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
    <rss2lj:owner xmlns:rss2lj="http://rss2lj.net/NS">bizikov</rss2lj:owner>
</feed>